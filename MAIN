package com.uninabiogarden.oobd68;

import com.uninabiogarden.oobd68.boundary.LoginBoundary;
import javax.swing.SwingUtilities;

public class Main {
    static void main(String[] sargs) {

        System.out.println("Avvio applicazione Unina BioGarden...");

        // Avvia l'applicazione nel Thread grafico (EDT)
        SwingUtilities.invokeLater(() -> {
            // Creiamo la prima finestra (Login) e la rendiamo visibile
            new LoginBoundary().setVisible(true);
        });
    }
}
